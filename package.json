{
  "name": "@cloudflare/sandbox-minimal-example",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "A minimal example of using the Sandbox SDK",
  "scripts": {
    "deploy": "pnpm run migrate:remote && pnpm run build && wrangler deploy",
    "dev": "pnpm run migrate:local && wrangler dev",
    "start": "pnpm run dev",
    "cf-typegen": "wrangler types",
    "typecheck": "tsc --noEmit",
    "generate": "drizzle-kit generate",
    "migrate:remote": "pnpm run generate && wrangler d1 migrations apply DB --remote",
    "migrate:local": "pnpm run generate && wrangler d1 migrations apply DB --local",
    "build": "astro build && node scripts/augment-worker.js"
  },
  "devDependencies": {
    "@cloudflare/sandbox": "^0.7.0",
    "@cloudflare/workers-types": "^4.20260120.0",
    "@types/node": "^24.10.9",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.9.3",
    "wrangler": "^4.59.2"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/react": "^4.4.2",
    "@astrojs/tailwind": "^6.0.2",
    "@google/genai": "^1.37.0",
    "@hono/zod-openapi": "^1.2.0",
    "@octokit/rest": "^22.0.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.11",
    "class-variance-authority": "^0.7.1",
    "cloudflare": "^5.2.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "openai": "^6.16.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.25.1"
  }
}
